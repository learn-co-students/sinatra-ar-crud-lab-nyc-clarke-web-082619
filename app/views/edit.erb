

<form action="/articles/<%= @article.id %>" method="POST">
<input type=”hidden” name=”_method” value=”PATCH”>
Title: <input type=”text” id=title name=”article[title]” value=”<%= @article.title %>”><br>
Content: <input type=”text” id=content name=”article[content]” value=”<%= @article.content %>”><br>
<input type="submit" id="Submit">
</form>